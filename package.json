    "mkdirp": "^0.5.0",
    "nunjucks": "^1.1.0",
    "nunjucks-markdown": "^0.5.2",
    "q": "^1.0.1",
    "requirejs": "^2.1.15",
    "rimraf": "^2.2.8",
    "run-sequence": "^1.0.1",
    "through2": "^1.1.1",
    "gulp-zip": "^2.0.2"
  }
