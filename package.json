    "del": "^1.1.0",
    "gulp": "^3.8.8",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-cached": "^1.0.1",
    "gulp-debug": "^1.0.1",
    "gulp-filter": "^1.0.2",
    "gulp-if": "^1.2.5",
    "gulp-jscs": "^1.1.2",
    "gulp-jshint": "^1.8.5",
    "gulp-karma": "0.0.4",
    "gulp-less": "^2.0.1",
    "gulp-minify-html": "^0.1.6",
    "gulp-newer": "^0.5.0",
    "gulp-plumber": "^0.6.6",
    "gulp-prettify": "^0.3.0",
    "gulp-recess": "^1.1.1",
    "gulp-rename": "^1.2.0",
    "gulp-replace": "^0.5.0",
    "gulp-sourcemaps": "^1.2.4",
    "gulp-util": "^3.0.1",
    "inquirer": "^0.8.0",
    "jshint-stylish": "^1.0.0",
    "json-stable-stringify": "^1.0.0",
    "karma": "^0.12.24",
